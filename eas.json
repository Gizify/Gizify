{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://gizify-backend.vercel.app/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://gizify-backend.vercel.app/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://gizify-backend.vercel.app/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
